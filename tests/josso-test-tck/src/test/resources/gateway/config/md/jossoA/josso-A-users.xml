<!--
IdentityMemoryStore user definitions.
-->
<josso-users>
    <users>
        <user>
            <name>userA</name>
            <!-- Optionally, define some properties for this user -->
            <properties>
                <property>
                    <name>user.name</name>
                    <value>User A Name</value>
                </property>
                <property>
                    <name>user.lastName</name>
                    <value>User A Last Name</value>
                </property>
                <property>
                    <name>user.registrationDate</name>
                    <value>2004/09/11</value>
                </property>
                <property>
                    <name>email</name>
                    <value>userA@josso.org</value>
                </property>
            </properties>
            <!-- Optional comma sepparated set of roles, must be defined below -->
            <roles>roleA , roleB</roles>
        </user>

        <user>
            <name>sgonzalez</name>
            <!-- Optionally, define some properties for this user -->
            <properties>
                <property>
                    <name>user.name</name>
                    <value>Sebastian</value>
                </property>
                <property>
                    <name>user.lastName</name>
                    <value>Gonzalez Oyuela</value>
                </property>
                <property>
                    <name>user.registrationDate</name>
                    <value>2004/09/11</value>
                </property>
                <property>
                    <name>email</name>
                    <value>sgonzalez@josso.org</value>
                </property>
            </properties>
            <!-- Optional comma sepparated set of roles, must be defined below -->
            <roles>roleA , roleB</roles>
        </user>

        <user>
            <name>userB</name>
            <!-- Optionally, define some properties for this user -->
            <properties>
                <property>
                    <name>user.name</name>
                    <value>User B Name</value>
                </property>
                <property>
                    <name>user.lastName</name>
                    <value>User B Last Name</value>
                </property>
                <property>
                    <name>user.registrationDate</name>
                    <value>2004/09/10</value>
                </property>
                <property>
                    <name>email</name>
                    <value>userB@josso.org</value>
                </property>
            </properties>
            <!-- Optional comma sepparated set of roles, must be defined below -->
            <roles>roleB</roles>
        </user>
        <user>
            <name>userC</name>
            <!-- Optionally, define some properties for this user -->
            <properties>
                <property>
                    <name>user.name</name>
                    <value>User C Name</value>
                </property>
                <property>
                    <name>user.lastName</name>
                    <value>User C Last Name</value>
                </property>
                <property>
                    <name>user.registrationDate</name>
                    <value>2005/01/1</value>
                </property>
            </properties>
            <!-- Optional comma sepparated set of roles, must be defined below -->
            <roles>roleA</roles>
        </user>
    </users>
    <!-- Roles definitions -->
    <roles>
        <role>
            <name>roleA</name>
        </role>
        <role>
            <name>roleB</name>
        </role>

    </roles>
</josso-users>
