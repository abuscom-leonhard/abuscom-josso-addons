<!--
IdentityMemoryStore user definitions.
-->
<josso-users>
    <users>
        <user>
            <name>marissa</name>
            <!-- Optional comma sepparated set of roles, must be defined below -->
            <roles>ROLE_USER , ROLE_SUPERVISOR, role1, role2</roles>
        </user>
        <user>
            <name>dianne</name>
            <!-- Optional comma sepparated set of roles, must be defined below -->
            <roles>ROLE_USER,role1</roles>
        </user>
        <user>
            <name>scott</name>
            <!-- Optional comma sepparated set of roles, must be defined below -->
            <roles>ROLE_USER,role2</roles>
        </user>

        <user>
            <name>peter</name>
            <!-- Optional comma sepparated set of roles, must be defined below -->
            <roles>ROLE_USER</roles>
        </user>

        <user>
            <name>bill</name>
            <!-- Optional comma sepparated set of roles, must be defined below -->
            <roles>ROLE_USER</roles>
        </user>

        <user>
            <name>bob</name>
            <!-- Optional comma sepparated set of roles, must be defined below -->
            <roles>ROLE_USER</roles>
        </user>

        <user>
            <name>jane</name>
            <!-- Optional comma sepparated set of roles, must be defined below -->
            <roles>ROLE_USER</roles>
        </user>




    </users>

    <!-- Roles definitions -->
    <roles>
        <role>
            <name>ROLE_USER</name>
        </role>
        <role>
            <name>ROLE_SUPERVISOR</name>
        </role>

        <role>
            <name>role1</name>
        </role>
        <role>
            <name>role2</name>
        </role>


    </roles>
</josso-users>
