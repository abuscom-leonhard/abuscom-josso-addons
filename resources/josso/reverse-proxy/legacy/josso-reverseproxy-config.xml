<reverse-proxy>
    <proxy-contexts>
        <proxy-context>

	    <name>partnerapp 1</name>
            <context>/partnerapp</context>
            <forward-host>http://localhost:8080</forward-host>
            <forward-uri>/partnerapp</forward-uri>
        </proxy-context>

        <proxy-context>
    	    <name>partnerapp 2</name>
            <context>/partnerapp2</context>
            <forward-host>http://localhost:8080</forward-host>
            <forward-uri>/partnerapp2</forward-uri>
        </proxy-context>

    </proxy-contexts>
</reverse-proxy>
